{
  "name": "chrome-angular-debug-bar",
  "version": "0.1.0",
  "description": "",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injector.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "injected.js"
  ],
  "minimum_chrome_version": "21",
  "externally_connectable": {
    "ids": ["*"]
  }
}
